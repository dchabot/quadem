# Database for APS quad electrometer
#   Mark Rivers
#   February 24, 2003

grecord(mbbo,"$(P)$(EM)Gain") {
    field(DESC,"Gain")
    field(PINI,"YES")
    field(ZRVL,"0")
    field(ZRST,"1.00")
    field(ONVL,"7")
    field(ONST,"2.51")
    field(TWVL,"6")
    field(TWST,"2.93")
    field(THVL,"5")
    field(THST,"3.52")
    field(FRVL,"4")
    field(FRST,"4.40")
    field(FVVL,"3")
    field(FVST,"5.87")
    field(SXVL,"2")
    field(SXST,"8.80")
    field(SVVL,"1")
    field(SVST,"17.6")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) GAIN")
}

grecord(ao,"$(P)$(EM)ConversionTime") {
    field(DESC,"Conversion Time (usec)")
    field(PINI,"YES")
    field(DRVL,"615.")
    field(LOPR,"615.")
    field(DRVH,"13107.8")
    field(HOPR,"13107.8")
    field(PREC,"1")
    field(VAL,"615.")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) CONVERSION")
}

grecord(ao,"$(P)$(EM)Period") {
    field(DESC,"Period")
    field(PINI,"YES")
    field(PREC,"0")
    field(VAL,"-1")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) PERIOD")
}

grecord(ao,"$(P)$(EM)Pulse") {
    field(DESC,"Pulse")
    field(PINI,"YES")
    field(PREC,"0")
    field(VAL,"1024")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) PULSE")
}

grecord(ao,"$(P)$(EM)Go") {
    field(DESC,"Go")
    field(PINI,"YES")
    field(PREC,"0")
    field(VAL,"0")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) GO")
}

grecord(seq,"$(P)$(EM)Initialize") {
    field(DESC,"Initialize")
    field(PINI,"YES")
    field(LNK1,"$(P)$(EM)Gain.PROC PP MS")
    field(DLY1,".01")
    field(LNK2,"$(P)$(EM)Pulse.PROC PP MS")
    field(DLY2,".01")
    field(LNK3,"$(P)$(EM)Period.PROC PP MS")
    field(DLY3,".01")
    field(LNK4,"$(P)$(EM)ConversionTime.PROC PP MS")
    field(DLY4,".01")
    field(LNK5,"$(P)$(EM)Go.PROC PP MS")
    field(DLY5,".01")
}

grecord(ao,"$(P)$(EM)Offset1") {
    field(DESC,"Offset")
    field(PINI,"YES")
    field(DRVL,"-65535")
    field(LOPR,"-65535")
    field(DRVH,"65535")
    field(HOPR,"65535")
    field(PREC,"0")
    field(VAL,"4096")
    field(DTYP, "quadEM")
    field(OUT,"#C$(CARD) S0 @$(SERVER) OFFSET")
}

