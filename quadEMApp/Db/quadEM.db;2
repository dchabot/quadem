grecord(ai,"$(P)$(EM)Average") {
    field(DESC,"Compute average")
    field(PINI,"NO")
    field(SCAN,"1 second")
    field(DTYP, "quadEM")
    field(INP,"#C$(CARD) S0 @(SERVER) AVERAGE")
    field(FLNK,"$(P)$(EM)Current1")
}
grecord(ai,"$(P)$(EM)Current1") {
    field(DESC,"Diode 1 current")
    field(PINI,"NO")
    field(LOPR,"0")
    field(HOPR,"65535")
    field(PREC,"0")
    field(DTYP, "quadEM")
    field(INP,"#C$(CARD) S0 @(SERVER) CURRENT")
    field(FLNK,"$(P)$(EM)Current2")
}

